package com.interview;

public class ArmstrongNumber {
	public static void main(String[] args) {
		int no = 153;
		int temp = no;
		int length = 0;

		while (temp != 0) {
			temp = temp / 10;
			length = length + 1;
		}

		int temp2 = no;
		int remainder = 0;

		while (temp2 != 0) {
			remainder = temp2 / 10;
			int cube = 1;
			for (int i = 1; i <= length; i++) {
				cube = cube * remainder;
			}

		}

	}
}
