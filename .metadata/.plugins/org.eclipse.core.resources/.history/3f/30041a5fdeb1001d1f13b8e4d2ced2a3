package com.interview;

import java.util.Arrays;

public class ArraySort {
	public static void main(String[] args) {
		
		int[] input = {2,5,9,3,6};
		for(int i=0;i<input.length;i++) {
			for(int j=0;j<input.length;j++) {
				if(input[i] > input[j]) {
					int temp =input[i];
					input[i] = input[j];
					input[j] = temp;
				}
			}
			System.out.println(Arrays.toString(input));
		}
		}
	}
