package com.interview;

public class Demo {
	public static void main(String[] args) {
		reverseString("Nilim");
	}

	private static void reverseString(String string) {
		String rev = "";
		for (int i = string.length() - 1; i >= 0; i--) {
			rev += string.charAt(i);
		}
		System.out.println(rev);

	}
}
